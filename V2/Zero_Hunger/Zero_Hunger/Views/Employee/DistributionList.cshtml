@model IEnumerable<ZeroHunger.Ef.Models.Distribution>

@{
    ViewBag.Title = "Distribution List";
    Layout = "~/Views/Shared/_LayoutEmployee.cshtml";
}

<h2>DistributionList</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FoodName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ResturantName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CollectionDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FoodName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ResturantName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmployeeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CollectionDate)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.DId }) 
        </td>
    </tr>
}

</table>
